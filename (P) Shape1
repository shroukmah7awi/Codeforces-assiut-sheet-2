
using namespace std;
#include <iostream>
#include <algorithm>
#include <vector>
#include <string>
#include <iomanip> 
#include<cmath>
#include<bitset>



int main()
{
	int n;
	cin >> n;

	for (int i = 1; i <= n; i++)
	{
		
		for (int j = n; j>= i; j--)
		{
			cout << "*";
		}
		cout << "\n";
	}

}
