int main()
{
 
 
	
	
	int pass ;
 
 
	while (1)
	{
		cin >> pass;
		if (pass == 1999)
		{
			cout << "Correct"<<endl;
			break;
		}
		else
		{
			cout << "Wrong" << endl;
		}
	}
}
