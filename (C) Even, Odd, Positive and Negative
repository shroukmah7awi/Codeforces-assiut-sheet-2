using namespace std;
#include <iostream>
#include <algorithm>
#include <vector>
#include <iomanip> 
#include<cmath>
#include<bitset>
int main()
{
    
    int n;
    long long numbers;
    cin >> n;
    int even = 0, odd = 0, pos = 0, neg = 0;
    
    for (int i = 0; i < n; i++)
    {
        cin >> numbers;
 
        if (numbers > 0)
        {
            pos++;
 
        }
        else if (numbers<0)
        {
            neg++;
 
        }
 
 
        if (numbers % 2 == 0)
        {
            even++;
 
        }
        else
        {
            odd++;
 
        }
    }
      cout << "Even: " << even << endl;
      cout << "Odd: " << odd << endl;
      cout << "Positive: " << pos<<endl;
      cout << "Negative: " << neg<<endl;
     
}
        
 
