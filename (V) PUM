
using namespace std;
#include <iostream>
#include <algorithm>
#include <vector>
#include <string>
#include <iomanip> 
#include<cmath>
#include<bitset>


int main()
{
	int n;
	cin >> n ;
	int count = 1;


	for (int i = 0; i < n; i++)
	{
		for (int j = count; j <= count + 3; j++)
		{
			if (j % 4 == 0)
			{
				cout << " PUM"<<endl;
				count += 4;
				break;
			}
			else
			{
				cout << j<<" ";
			}
		}
	}
	

}
