
using namespace std;
#include <iostream>
#include <algorithm>
#include <vector>
#include <string>
#include <iomanip> 
#include<cmath>
#include<bitset>



int main()
{
	char c;
	int n;
	int numbers;

	cin >> c >> n ;

	for (int i = 1; i <= n; ++i)
	{
		cin >> numbers;
		for (int j = 0; j < numbers; ++j)
		{
			cout << c;
		}
		cout << "\n";
	}

}
	
